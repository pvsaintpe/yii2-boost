{
  "name": "pvsaintpe/yii2-boost",
  "description": "Extension",
  "keywords": [
    "yii2",
    "gii",
    "code generator"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Veselov",
      "email": "pvsaintpe@icloud.com"
    }
  ],
  "require": {
    "yiisoft/yii2": ">=2.0.4",
    "firephp/firephp-core": "*",
    "psr/log": "*",
    "pvsaintpe/yii2-db-plus": "*"
  },
  "autoload": {
    "psr-4": {
      "pvsaintpe\\boost\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
